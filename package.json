{
  "name": "systemjs-tools",
  "version": "2.0.1",
  "description": "(dev)tools for working with SystemJS",
  "main": "dist/index.js",
  "browser": "dist/client.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel lib -d dist && chmod +x ./dist/cli.js",
    "dev": "babel -w lib -d dist",
    "prepublish": "npm run build",
    "cli": "node ./dist/cli.js"
  },
  "registry": "npm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexisvincent/systemjs-tools.git"
  },
  "bin": {
    "systemjs": "./dist/cli.js"
  },
  "keywords": [
    "JSPM",
    "dev",
    "tools",
    "devtools",
    "systemjs"
  ],
  "author": "Alexis Vincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexisvincent/systemjs-tools/issues"
  },
  "homepage": "https://github.com/alexisvincent/systemjs-tools#readme",
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-env": "^1.1.6",
    "jspm": "beta"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "chokidar": "^1.6.1",
    "commander": "^2.9.0",
    "deepmerge": "^1.3.1",
    "express": "^4.14.0",
    "graceful-fs": "^4.1.11",
    "http-proxy": "^1.13.0",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "ncp": "^2.0.0",
    "rxjs": "^5.0.3",
    "socket.io": "^1.6.0",
    "spdy": "^3.4.4",
    "spdy-keys": "^0.0.0",
    "systemjs-builder": "^0.16.4",
    "systemjs-config-builder": "^0.0.9"
  }
}
